  {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "es6": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb",
      "airbnb-base"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ]
    },
    "globals": {
      "require": true,
      "module": true,
      "process": true
    }
  }